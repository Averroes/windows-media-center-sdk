                                    Value="0,0,0"
                                    Interpolation="Bezier"
                                    RelativeTo="Current"/>
                  <PositionKeyframe Time="30"
                                    Value="600,600,600"
                                    Interpolation="Bezier"
                                    RelativeTo="Current"/>
                  <PositionKeyframe Time="60"
                                    Value="-600,-600,-600"
                                    Interpolation="Bezier"
                                    RelativeTo="Current"/>
