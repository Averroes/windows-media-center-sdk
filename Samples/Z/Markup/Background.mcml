          <Graphic Content="resx://Z/Z.Resources/BackgroundItem1">
            <Animations>

              <Animation Loop="-1"
                         CenterPointPercent=".5,.5,.5">
                <Keyframes>
                  <AlphaKeyframe Time="0"
                                 Value="0"
                                 Interpolation="Bezier"
                                 RelativeTo="Current"/>
                  <AlphaKeyframe Time="7.5"
                                 Value=".3"
