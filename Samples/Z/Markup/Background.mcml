                                 RelativeTo="Current"/>
                  <AlphaKeyframe Time="15"
                                 Value="0"
                                 Interpolation="Bezier"
                                 RelativeTo="Current"/>

                  <RotateKeyframe Time="0"
                                  Value="0deg;.5,.5,.5"
                                  Interpolation="Bezier"
                                  RelativeTo="Current"/>
                  <RotateKeyframe Time="60"
                                  Value="360deg;.5,.5,.5"
                                  Interpolation="Bezier"
                                  RelativeTo="Current"/>

                  <ScaleKeyframe Time="0"
                                 Value="1,1,1"
                                 Interpolation="Bezier"
                                 RelativeTo="Current"/>
                  <ScaleKeyframe Time="15"
                                 Value=".5,.5,.5"
